ZSH_AUTOSUGGEST_HIGHLIGHT_STYLE='fg=240'
ZSH_AUTOSUGGEST_USE_ASYNC=1
ZSH_AUTOSUGGEST_MANUAL_REBIND=1  # This can be a big boost to performance, but you'll need to handle re-binding yourself  https://github.com/zsh-users/zsh-autosuggestions#disabling-automatic-widget-re-binding

