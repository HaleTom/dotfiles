  atload"[[ current_theme.zsh -nt themes/$FAST_THEME_NAME.ini ]] || fast-theme $FAST_THEME_NAME > /dev/null" \
  atload'FAST_HIGHLIGHT[chroma-man]=  # Prevent horrible purple background' \
  atclone'(){local f;cd -q →*;for f (*~*.zwc){zcompile -Uz -- ${f}};}' atpull'%atclone  # compile chroma directory' \
  compile'.*fast*~*.zwc' nocompletions \
  for 'zdharma-continuum/fast-syntax-highlighting'

typeset -gA FAST_HIGHLIGHT_STYLES FAST_HIGHLIGHT
