#!/bin/bash
#      ^----- get shellcheck hints based on bash
# https://github.com/koalaman/shellcheck/issues/809
#
# shellcheck disable=SC1090 # sourced files with variables

# FIXME: g logda G inserts GG and fails  (bisect)

# Zinit
# Wiki:    https://zdharma-continuum.github.io/zinit/wiki/
# Github:  https://github.com/zdharma-continuum/zinit
#
# Debug:   https://github.com/zdharma-continuum/zinit#tracking-of-the-active-session

# Setup Zinit - https://github.com/zdharma-continuum/zinit#customizing-paths
declare -A ZINIT
ZINIT[NO_ALIASES]=1  # Remove Zinit's useless aliases -- 'zi' conflicts with z.lua and zoxide
ZINIT[HOME_DIR]=$XDG_DATA_HOME/zinit/
ZINIT[ZCOMPDUMP_PATH]=$ZSH_COMPDUMP

### Added by Zinit's installer
if [[ ! -f $HOME/.local/share/zinit/zinit.git/zinit.zsh ]]; then
    print -P "%F{33} %F{220}Installing %F{33}ZDHARMA-CONTINUUM%F{220} Initiative Plugin Manager (%F{33}zdharma-continuum/zinit%F{220})…%f"
    command mkdir -p "$HOME/.local/share/zinit" && command chmod g-rwX "$HOME/.local/share/zinit"
    { command git clone https://github.com/zdharma-continuum/zinit "$HOME/.local/share/zinit/zinit.git" && \
        print -P "%F{33} %F{34}Installation successful.%f%b"; } || \
        print -P "%F{160} The clone has failed.%f%b"
fi

source "$HOME/.local/share/zinit/zinit.git/zinit.zsh"
autoload -Uz _zinit
(( ${+_comps} )) && _comps['zinit']=_zinit

# Pending issue: (search issue in zshrc also)
# https://github.com/zdharma-continuum/zinit-module/issues/8

# Profile all "source" commands (but beware of ~/bin/source-file)
# zinit module build
# zpmod source-study


# Load a few important annexes, without Turbo
# (this is currently required for annexes)
zinit light-mode for \
    zdharma-continuum/zinit-annex-as-monitor \
    zdharma-continuum/zinit-annex-bin-gem-node \
    zdharma-continuum/zinit-annex-patch-dl \
    zdharma-continuum/zinit-annex-rust

### End of Zinit's installer chunk

# Default options for zinit
_zinit_defaults=(zinit wait light-mode depth'1')


# Run these commands asynchronously via null plugin
__zinit_deferred () {
  # Load pip completions if pip installed
  # Alternatively, as suggested in https://web.archive.org/web/20201008014818/https://github.com/zdharma/zinit/issues/68#issuecomment-410531439
  # shellcheck disable=2154  # $commands not assigned
  [[ ${commands[pip]} ]] && eval "$(pip completion --zsh)"

  # Source fzf files
  # Needs to be sourced after zsh-vim-mode to have ^R work
  if [[ ${commands[fzf]} ]]; then  # fzf is installed
    source_first "/usr/share/fzf/completion.zsh" "/usr/local/opt/fzf/shell/completion.zsh"
    source_first "/usr/share/fzf/key-bindings.zsh" "/usr/local/opt/fzf/shell/key-bindings.zsh"
  fi
  # Setup my aliases after any turbo-loaded plugins
  _source_file "$XDG_CONFIG_HOME/bash/aliases"
  # echo finished running deferred function
  unfunction __zinit_deferred  # remove thyself
}


# clockify-cli completions
# Awaiting https://aur.archlinux.org/packages/clockify-cli-bin#comment-1060465
"${_zinit_defaults[@]}" id-as'clockify-completion' has'clockify-cli' \
    null completions \
    atclone'/usr/bin/clockify-cli completion zsh >| _completions' \
    atpull'%atclone' run-atpull \
    src'_completions' \
  for zdharma-continuum/null


# Friendly bindings for ZSH's vi mode
# https://github.com/softmoth/zsh-vim-mode
MODE_CURSOR_VICMD="green block"
MODE_CURSOR_VIINS="#20d08a blinking bar"
MODE_CURSOR_SEARCH="#ff00ff steady underline"
# How to manage bindkeys in plugins - eg withold the ^R binding
# https://web.archive.org/web/20201008014818/https://github.com/zdharma/zinit/issues/69
zinit ice wait depth"1" trackbinds bindmap'^R -> hold'  # ^R conflicts with fzf
zinit light 'softmoth/zsh-vim-mode'


# Additional completion definitions for Zsh
# https://github.com/zsh-users/zsh-completions
# Have zinit manage completions  # https://github.com/zdharma-continuum/zinit#completions
"${_zinit_defaults[@]}" completions atpull'zinit cclear -q && zinit creinstall -q .' \
  atinit"  # Source: https://devstate.de/uncategorized/pimp-my-terminal-with-zinit-2022/#:~:text=zsh%2Dusers/zsh%2Dcompletions
    zstyle ':completion:*' completer _expand _complete _ignored _approximate
    zstyle ':completion:*' matcher-list 'm:{a-z}={A-Z}'
    zstyle ':completion:*' menu select=2
    zstyle ':completion:*' select-prompt '%SScrolling active: current selection at %p%s'
    zstyle ':completion:*:*:*:*:processes' command 'ps -u $USER -o pid,user,comm,cmd -w -w'
    zstyle ':completion:*:descriptions' format '-- %d --'
    zstyle ':completion:*:processes' command 'ps -au$USER'
    zstyle ':completion:complete:*:options' sort false" \
for 'zsh-users/zsh-completions'


# fzf required
if [[ ${commands[fzf]} ]]; then # Don't assume that fzf is installed
  # fzf-tab
  # Replace zsh's default completion selection menu with fzf!
  # https://github.com/Aloxaf/fzf-tab
  "${_zinit_defaults[@]}" for Aloxaf/fzf-tab

  # fzf-git.sh -- key bindings for Git objects, powered by fzf
  # https://github.com/junegunn/fzf-git.sh
  # Awaiting bell issue: https://github.com/junegunn/fzf-git.sh/issues/88
  "${_zinit_defaults[@]}" atinit"FZF_GIT_CAT=$HOME/bin/pager" \
    for junegunn/fzf-git.sh

  # Preview directory's content with eza when completing cd
  # shellcheck disable=SC2016  # SC2016 (info): Expressions don't expand in single quotes
  [[ ${commands[eza]} ]] && zstyle ':fzf-tab:complete:cd:*' fzf-preview 'eza -1 --color=always $realpath'
  # From: https://github.com/Aloxaf/fzf-tab?tab=readme-ov-file#configure
  zstyle ':fzf-tab:*' fzf-bindings 'space:accept' 'ctrl-a:toggle-all'  # like --bind=space:accept
  # NOTE: fzf-tab does not follow FZF_DEFAULT_OPTS by default
  zstyle ':fzf-tab:*' fzf-flags --color=fg:1,fg+:2
  # Switch group using `<` and `>`
  zstyle ':fzf-tab:*' switch-group '<' '>'
  # To make fzf-tab follow FZF_DEFAULT_OPTS (not FZF_DEFAULT_OPTS_FILE)
  # NOTE: This may lead to unexpected behavior since some flags break this plugin.
  # See https://github.com/Aloxaf/fzf-tab/issues/455: [BUG] Using --tmux in an exported FZF_DEFAULT_OPTS breaks ftb-tmux-popup
  zstyle ':fzf-tab:*' use-fzf-default-opts yes

  # https://github.com/Aloxaf/fzf-tab/wiki/Configuration#universal
  zstyle ':fzf-tab:*' fzf-command ftb-tmux-popup
  zstyle ':fzf-tab:*' accept-line enter
  # To disable continuous-trigger for a specific command, use: zstyle ':fzf-tab:*<CMD>*' continuous-trigger ''
  zstyle ':fzf-tab:*' continuous-trigger '/'
  zstyle ':fzf-tab:complete:*' popup-pad 30 0  # for ftb-tmux-popup
  zstyle ':fzf-tab:complete:_zlua:*' query-string input

  # [[ ${commands[eza]} ]] && zstyle ':fzf-tab:complete:cd:*' extra-opts --preview=$extract'eza -1 --color=always ${~ctxt[hpre]}$in'
  # zstyle ':fzf-tab:complete:kill:argument-rest' extra-opts --preview=$extract'ps --pid=$in[(w)1] -o cmd --no-headers -w -w' --preview-window=down:3:wrap


  # A collection of fzf-tab's sources taken from: https://github.com/Aloxaf/fzf-tab/wiki/Preview#preview-gallery
  # https://github.com/Freed-Wu/fzf-tab-source
  "${_zinit_defaults[@]}" for 'Freed-Wu/fzf-tab-source'
fi  # have fzf


# Add zsh's missing hooks, and support for user-defined hooks
# eg zle-line-init of the Special Wigets in `man zshzle`
# https://github.com/zsh-hooks/zsh-hooks'
# No 'wait' ice as it's used immediately in keyboard setup
zinit ice depth"1" # pick"zsh-hooks.plugin.zsh"
zinit light 'zsh-hooks/zsh-hooks'


# Print zsh's hooks
# https://github.com/agkozak/zhooks
"${_zinit_defaults[@]}" wait trigger-load'!zhooks' for 'agkozak/zhooks'


# Bracketed Paste mode (uses zsh-hooks)
# My merged PR with small unaddressed comment: https://github.com/robbyrussell/oh-my-zsh/pull/7887
