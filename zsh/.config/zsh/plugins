zinit ice wait depth"1"
zinit snippet OMZ::plugins/safe-paste/safe-paste.plugin.zsh


# Auto-close and delete matching delimiters in zsh
# https://github.com/hlissner/zsh-autopair
"${_zinit_defaults[@]}" atload'unset "AUTOPAIR_PAIRS[ ]"' \
for hlissner/zsh-autopair
# By default, when typing '[ x ]' the 2nd space doesn't consume the added space.
