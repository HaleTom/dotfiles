  atload"bindkey '^_' autosuggest-execute
         bindkey '^ ' autosuggest-accept  # Ctrl-<SPACE>" \
  for zsh-users/zsh-autosuggestions

ZSH_AUTOSUGGEST_STRATEGY=(match_prev_cmd completion)  # Match on previous history command also.  https://github.com/zsh-users/zsh-autosuggestions#suggestion-strategy
